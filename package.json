{
  "name": "nevem-web",
  "version": "1.0.0",
  "description": "nevem-web",
  "main": "src/index.js",
  "scripts": {
    "lint": "npm run lint:js",
    "lint:eslint": "eslint --ignore-path .eslintignore",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:staged": "lint-staged",
    "clean": "rimraf www/*",
    "hot": "npm run clean && webpack --env=dev --watch",
    "hot:prod": "npm run clean && webpack --env=prod --watch",
    "prepare": "npm run clean && node config && webpack --env=prod",
    "build": "npm run clean && node config && webpack --env=prod",
    "build:dev": "npm run clean && node config && webpack --env=dev",
    "build:test": "npm run clean && node config && webpack --env=test",
    "start": "cross-env NODE_ENV=development node server",
    "start:test": "cross-env NODE_ENV=test node server",
    "start:tunnel": "cross-env NODE_ENV=development ENABLE_TUNNEL=true node server",
    "start:production": "npm run test && npm run build && npm run start:prod",
    "start:prod": "cross-env NODE_ENV=production node server",
    "pretest": "npm run test:clean && npm run lint",
    "test:clean": "rimraf ./coverage",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "install": "napa"
  },
  "napa": {
    "flot": "flot/flot#v0.8.3",
    "jquery.flot.spline": "miloszfalinski/jquery.flot.spline",
    "ika.jvectormap": "kakirigi/ika.jvectormap",
    "nestable": "dbushell/Nestable",
    "dataTables.fontAwesome": "https://cdn.datatables.net/plug-ins/1.10.10/integration/font-awesome/dataTables.fontAwesome.css",
    "jqueryui-touch-punch": "furf/jquery-ui-touch-punch"
  },
  "lint-staged": {
    "*.js": "lint:eslint"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/*.test.{js,jsx}",
      "!src/*/RbGenerated*/*.{js,jsx}",
      "!src/app.js"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 1,
        "branches": 1,
        "functions": 1,
        "lines": 1
      }
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      ".*\\.(css|less|styl|scss|sass)$": "<rootDir>/supports/mocks/cssModule.js",
      ".*\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/supports/mocks/image.js"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/supports/testing/test-bundler.js",
    "testRegex": "tests/.*\\.test\\.js$"
  },
